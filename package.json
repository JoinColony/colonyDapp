    "scripts": {
        "contracts:deploy": "scripts/deploy_contracts.js",
        "dev": "scripts/start_all.js --foreground",
        "flow": "flow check",
        "flow:ci": "scripts/flow_ci.sh",
        "ganache": "scripts/start_ganache.js",
        "lint": "eslint --fix 'src/**/*.{js,jsx}' 'integration-testing/**/*.js' 'cypress/**/*.js'",
        "lint:ci": "eslint --format junit -o 'reports/eslint/eslint-results.xml' 'src/**/*.{js,jsx}' 'integration-testing/**/*.js' 'cypress/**/*.js'",
        "provision": "scripts/provision_submodules.sh",
        "scaffold": "plop",
        "styleguide": "styleguidist server",
        "styleguide:build": "styleguidist build",
        "styleguide:deploy": "gh-pages -d styleguide -r git@github.com:JoinColony/colonyUI.git",
        "stylelint": "stylelint --fix \"src/**/*.css\"",
        "stylelint:ci": "stylelint --custom-formatter './node_modules/stylelint-formatter-relative-junit' \"src/**/*.css\" > reports/stylelint/stylelint-results.xml",
        "test": "jest --config=jest.conf.json",
        "test:ci": "jest --clearCache && NODE_ENV=test jest --ci --coverage --verbose=true --config=jest.conf.json --reporters='jest-junit'",
        "test:integration": "ava",
        "test:integration:ci": "yarn test:integration --tap | tap-xunit > reports/ava/ava-results.xml",
        "truffle": "yarn --cwd ./src/lib/colonyNetwork truffle",
        "trufflepig": "scripts/start_trufflepig.js",
        "test:e2e": "rimraf .dappPids.json && scripts/start_all.js & wait-on .dappPids.json && scripts/run_cypress_based_on_browser.sh; scripts/stop_all.js",
        "test:e2e:ci": "scripts/start_all.js & wait-on .dappPids.json && cypress run --browser chrome",
        "webpack": "webpack-dev-server --https --key ./ssl/localhost+2-key.pem --cert ./ssl/localhost+2.pem --port 9090 --config webpack.config.js",
        "webpack:build": "webpack-cli --config webpack.config.js",
        "wssproxy": "scripts/start_wss_proxy.js"
    },
