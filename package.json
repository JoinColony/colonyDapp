    "scripts": {
        "contracts:deploy": "scripts/deploy_contracts.js",
        "dev": "scripts/start_all.js --foreground",
        "ganache": "scripts/start_ganache.js",
        "graphql:generate": "graphql-codegen --config codegen.yml",
        "lint": "eslint --fix 'src/**/*.{ts,tsx}' 'cypress/**/*.js'",
        "lint:ci": "eslint --format junit -o 'reports/eslint/eslint-results.xml' 'src/**/*.{ts,tsx}' 'cypress/**/*.js'",
        "provision": "scripts/provision_submodules.sh",
        "styleguide": "styleguidist server",
        "styleguide:build": "styleguidist build",
        "styleguide:deploy": "gh-pages -d styleguide -r git@github.com:JoinColony/colonyUI.git",
        "stylelint": "stylelint --fix \"src/**/*.css\"",
        "stylelint:ci": "stylelint --custom-formatter './node_modules/stylelint-formatter-relative-junit' \"src/**/*.css\" > reports/stylelint/stylelint-results.xml",
        "test": "jest --config=jest.conf.json",
        "test:ci": "jest --clearCache && NODE_ENV=test jest -w 1 --ci --verbose=true --config=jest.conf.json --reporters='jest-junit'",
        "test:e2e": "rimraf .dappPids.json && scripts/start_all.js & wait-on .dappPids.json && scripts/run_cypress_based_on_browser.sh; scripts/stop_all.js",
        "test:e2e:ci": "yarn dev & wait-on .dappPids.json && cypress run --browser chrome",
        "truffle": "yarn --cwd ./src/lib/colonyNetwork truffle",
        "trufflepig": "scripts/start_trufflepig.js",
        "typecheck": "tsc --noEmit",
        "webpack": "webpack-dev-server --config webpack.dev.js --port 9090",
        "webpack:build": "webpack-cli --config webpack.dev.js",
        "webpack:build:prod": "NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production webpack-cli --config webpack.prod.js",
        "wssproxy": "scripts/start_wss_proxy.js"
    },
